<h1>Recommendation Test Analysis</h1>

<div class="user-form">
	<%= error_messages_for 'test_results' %>
	
	<fieldset>
			<legend>Choose Recommended Course</legend>
			<% form_tag :action => 'get_recommendations_by_date' do %>
			<p>
				<label>Math</label>
				<% course_list =  ["20","22","58","60","65","70","76","95","97","105","111","112","211","213"] %>
				<%= select_tag "Course", options_for_select(course_list)%>
			</p>
				<% @test_list =  ["A","B","C","D","E1","E2","F","G","H","I"] %>
			<%= submit_tag "Get Score Distributions", :class => "submit" %>
			<% end %>
	</fieldset>
</div>
